class Square:
    def CalculateArea(self):
        print("Enter side:")
        self.l = float(input())
        area = self.l * self.l
        print("Area of square is=%f" % (area))

    def CalculatePerimeter(self):
        perimeter = 4 * (self.l)
        print("Perimeter of square is=%f" % (perimeter))

c = Square()
c.CalculateArea()
c.CalculatePerimeter()
